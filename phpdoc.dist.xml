<?xml version="1.0" encoding="UTF-8"?>
<phpdocumentor configVersion="3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://www.phpdoc.org">
    <title>IDN Converter - PHP Library</title>
    <paths>
        <output>docs</output>
        <cache>build/api-cache</cache>
    </paths>
    <version number="1.0.0">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
            <output>api</output>
            <ignore hidden="true" symlinks="true">
                <path>docs/**/*</path>
                <path>tests/**/*</path>
                <path>build/**/*</path>
                <path>vendor/**/*</path>
            </ignore>
            <extensions>
                <extension>php</extension>
            </extensions>
            <markers>
                <marker>TODO</marker>
                <marker>FIXME</marker>
            </markers>
            <default-package-name>CNIC</default-package-name>
        </api>
    </version>
</phpdocumentor>