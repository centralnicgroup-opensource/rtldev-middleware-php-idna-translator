{
    "name": "centralnicgroup-opensource/rtldev-middleware-php-idna-translator",
    "description": "PHP IDNA Translator library",
    "type": "library",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "CNIC\\IDNA\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CNIC\\IDNA\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --verbose tests/IDNATranslatorTest.php"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
