{
    "name": "centralnicgroup-opensource/rtldev-middleware-php-idna-translator",
    "description": "PHP IDNA Translator library",
    "type": "library",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.9"
    },
    "autoload": {
        "psr-4": {
            "CNIC\\IDNA\\": "src/IDNA/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CNIC\\IDNA\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "codefix": "phpcbf --ignore=\"*/vendor/*\" --standard=PSR12 -q src tests",
        "codecheck": "phpcs --ignore=\"*/vendor/*\" --standard=PSR12 -q src tests",
        "test": "XDEBUG_MODE=coverage phpunit --coverage-html=./reports/html"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
